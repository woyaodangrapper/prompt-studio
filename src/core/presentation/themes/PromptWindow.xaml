<Window x:Class="Maps.Studio.Themes.MapsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        xmlns:effects="clr-namespace:Themes.Effects"
        Title="MapsWindow" Height="450" Width="800">
    <WindowChrome.WindowChrome>
        <WindowChrome  GlassFrameThickness="0 1 0 0" />
    </WindowChrome.WindowChrome>
    <effects:WindowAcrylicBlur.WindowAcrylicBlur>
        <effects:WindowAcrylicBlur BlurColor="AliceBlue" Opacity="0.2" />
    </effects:WindowAcrylicBlur.WindowAcrylicBlur>
    <Grid>
    </Grid>
</Window>